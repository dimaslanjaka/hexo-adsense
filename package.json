  "description": "Hexo adsense support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": {
        "default": "./dist/index.cjs",
        "types": "./dist/index.d.ts"
      },
      "import": {
        "default": "./dist/index.mjs",
        "types": "./dist/index.d.mts"
      }
    }
  },
