{
  "name": "hexo-adsense",
  "version": "1.0.20",
  "description": "Hexo adsense support",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.d.ts,.tsx ./",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.d.ts,.tsx ./",
    "copy-files": "copyfiles -u 1 source/**/*.{html,css,js} lib/source/",
    "build": "rm -rf lib && tsc && npm run copy-files"
  },
  "files": [
    "lib",
    "*.md"
  ],
  "keywords": [
    "hexo",
    "auto",
    "adsense",
    "in-article-ads"
  ],
  "author": {
    "name": "Dimas Lanjaka",
    "email": "dimaslanjaka@gmail.com",
    "url": "https://github.com/dimaslanjaka"
  },
  "license": "ISC",
  "repository": "dimaslanjaka/hexo-adsense",
  "dependencies": {
    "bluebird": "^3.7.2",
    "hexo": "^5.4.0",
    "hexo-cli-extras": "file:packages/hexo-cli-extras",
    "hexo-extend-injector2": "file:packages/hexo-extend-injector2",
    "hexo-fs": "^3.1.0",
    "hexo-log": "^3.0.0",
    "md5-file": "^4.0.0",
    "minimatch": "^3.0.4",
    "moment": "^2.29.1",
    "object-assign": "^4.1.1",
    "underscore": "^1.13.1"
  },
  "devDependencies": {
    "@types/jsdom": "^16.2.13",
    "copyfiles": "github:calvinmetcalf/copyfiles",
    "eslint": "^7.32.0",
    "eslint-plugin-ejs": "^0.0.2",
    "jsdom": "^18.0.1"
  }
}