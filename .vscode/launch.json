  "version": "0.2.0",
  "configurations": [
    {
      "command": "hexo server --ssl",
      "name": "dev server",
      "request": "launch",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "hexo-adsense,hexo-adsense:*"
      },
      "cwd": "${workspaceFolder}/test/demo",
      "type": "node-terminal"
    },
    {
      "command": "hexo server --ssl",
      "name": "prod server",
      "request": "launch",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "hexo-adsense,hexo-adsense:*"
      },
      "cwd": "${workspaceFolder}/test/demo",
      "type": "node-terminal"
    }
  ]
}