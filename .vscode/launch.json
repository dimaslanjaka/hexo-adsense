  "version": "0.2.0",
  "configurations": [
    {
      "command": "cd ../../ && hexo clean && hexo server --development",
      "name": "hexo clean server from ../../",
      "request": "launch",
      "env": { "NODE_ENV": "development" },
      "type": "node-terminal"
    },
    {
      "command": "cd ../../ && hexo server --development",
      "name": "hexo server from ../../",
      "request": "launch",
      "env": { "NODE_ENV": "development" },
      "type": "node-terminal"
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Test Memorize",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/__test__/memorize.js"
    }
  ]
}
